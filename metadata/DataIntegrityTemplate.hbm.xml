<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.dataintegrity">

	<class name="DataIntegrityTemplate" table="dataintegrity_test">

		<id name="dataIntegrityId" type="int" column="id"
			unsaved-value="0">
				<generator class="native" />
		</id>

		<property name="name" type="java.lang.String" not-null="false" length="255" column="name" />
	
	</class>
</hibernate-mapping>
