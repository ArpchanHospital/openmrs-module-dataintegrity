<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.dataintegrity">

	<class name="DataIntegrityTemplate" table="dataintegrity_integrity_checks">

		<id name="integrityCheckId" type="int" column="dataintegrity_integrity_check_id">
				<generator class="native" />
		</id>

		<property name="integrityCheckName" type="java.lang.String" not-null="false" length="45" column="name" />
		<property name="integrityCheckSql" type="java.lang.String" not-null="false" length="100" column="sql" />
	</class>
</hibernate-mapping>
