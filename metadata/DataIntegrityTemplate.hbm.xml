<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.dataintegrity">

	<class name="DataIntegrityCheckTemplate" table="dataintegrity_integrity_checks">

		<id name="integrityCheckId" type="int" column="dataintegrity_integrity_check_id">
				<generator class="native" />
		</id>

		<property name="integrityCheckName" type="java.lang.String" not-null="true" length="45" column="check_name" />
		<property name="integrityCheckSql" type="java.lang.String" not-null="true" length="500" column="check_sql" />
		<property name="integrityCheckBaseForFailure" type="java.lang.Integer" not-null="true" column="check_base_for_failure" />
		<property name="integrityCheckScore" type="java.lang.Integer" not-null="true" column="check_score" />
	</class>
</hibernate-mapping>
